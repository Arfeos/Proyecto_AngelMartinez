<Window x:Class="proyecto_Final.Presentacion.reactivar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:proyecto_Final.Presentacion"
        mc:Ignorable="d"
        Title="reactivar" Height="450" Width="490" Loaded="Window_Loaded">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/./Recursos/estilo.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid Height="450" Style="{StaticResource papiro}" >
        <ListBox x:Name="listusu" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="163,37,45,37"/>
        <Button x:Name="btn_reactivar"  Content="ractivar" HorizontalAlignment="Left" Margin="40,84,0,0" VerticalAlignment="Top" Height="40" Width="90" Click="btn_reactivar_Click"/>
        <Button x:Name="btn_cacelar" Content="Cancelar" HorizontalAlignment="Left" Margin="40,139,0,0" VerticalAlignment="Top" Height="40" Width="90" Click="btn_cacelar_Click"/>

    </Grid>
</Window>
